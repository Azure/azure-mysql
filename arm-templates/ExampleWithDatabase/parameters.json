{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "administratorLogin": {
            "value": null
        },
        "administratorLoginPassword": {
            "value": null
        },
        "location": {
            "value": "West US"
        },
        "serverName": {
            "value": null
        },
        "skuCapacity": {
            "value": 2
        },
        "skuFamily": {
            "value": "Gen5"
        },
        "skuName": {
            "value": "GP_Gen5_2"
        },
        "skuSizeMB": {
            "value": 5120
        },
        "skuTier": {
            "value": "GeneralPurpose"
        },
        "version": {
            "value": "5.7"
        },
        "backupRetentionDays": {
            "value": 7
        },
        "geoRedundantBackup": {
            "value": "Disabled"
        },
        "databases": {
            "value": {
                "properties": [
                    {
                        "name": "sampledb1",
                        "charset": "utf8",
                        "collation": "utf8_general_ci"
                    },
                    {
                        "name": "sampledb2",
                        "charset": "ascii",
                        "collation": "ascii_general_ci"
                    }
                ]
            }
        }
    }
}