{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "administratorLogin": {
            "value": "adminuser"
        },
        "administratorLoginPassword": {
            "value": null
        },
        "serverName": {
            "value": "your-server-name"
        },
        "location": {
            "value": "southcentralus"
        },
        "skuName": {
            "value": "GP_Gen4_2"
        },
        "skuTier": {
            "value": "GeneralPurpose"
        },
        "skuCapacity": {
            "value": 2
        },
        "skuFamily": {
            "value": "Gen4"
        },
        "skuSizeMB": {
            "value": 5120
        },
        "version": {
            "value": "5.7"
        },
        "backupRetentionDays": {
            "value": 7
        },
        "geoRedundantBackup": {
            "value": "Disabled"
        },
        "virtualNetworkName": {
	    "value": "your-vnet-name"
	},
	"subnetName": {
	    "value": "your-subnet-name"
        },
        "virtualNetworkRuleName": {
	    "value": "AllowSubnet"
        }
    }
}