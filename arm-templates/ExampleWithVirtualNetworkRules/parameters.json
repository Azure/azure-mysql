"{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "administratorLogin": {
            "value": "adminlogin"
        },
        "administratorLoginPassword": {
            "value": "adminpassword"
        },
        "serverName": {
            "value": "your_server_name"
        },
        "location": {
            "value": "location"
        },
        "skuName": {
            "value": "GP_Gen4_2"
        },
        "skuTier": {
            "value": "GeneralPurpose"
        },
        "skuCapacity": {
            "value": 2
        },
        "skuFamily": {
            "value": "Gen4"
        },
        "skuSizeMB": {
            "value": 5120
        },
        "version": {
            "value": "5.7"
        },
        "backupRetentionDays": {
            "value": 7
        },
        "geoRedundantBackup": {
            "value": "Disabled"
        },
        "virtualNetworkName": {
	    "value": "your_vnet_name"
    },
    "vnetAddressPrefix": {
	    "value": "10.0.0.0/16"
    },
    "subnetPrefix": {
	    "value": "10.0.0.0/16"
    },    
	"subnetName": {
	    "value": "your_subnet_name"
        },
        "virtualNetworkRuleName": {
	    "value": "AllowSubnet"
        }
    }
}
